{
    "compilerOptions": {
        "outDir": "./",
        "allowJs": true,
        "target": "ESNext",
        "moduleResolution": "node",
        "module": "commonjs",
        "lib": [
            "ES2020"
        ],
        "esModuleInterop": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true
    },
    "include": [
        "*.ts"
    ],
    "exclude": []
}