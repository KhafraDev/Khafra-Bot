{
  "name": "khafra-bot",
  "version": "1.12.0",
  "description": "Discord Bot",
  "main": "build/src/index.js",
  "type": "module",
  "imports": {
    "#khaf/utility/*": "./src/lib/Utility/*",
    "#khaf/utility/commands/*": "./src/lib/common/*.js",
    "#khaf/utility/Discord.js": "./src/lib/types/Discord.js.js",
    "#khaf/database/*": "./src/Structures/Database/*",
    "#khaf/migration/*": "./src/lib/Migration/*",
    "#khaf/cache/*": "./src/lib/Cache/*",
    "#khaf/cooldown/*": "./src/Structures/Cooldown/*",
    "#khaf/Command": "./src/Structures/Command.js",
    "#khaf/Interaction": "./src/Structures/Interaction.js",
    "#khaf/Event": "./src/Structures/Event.js",
    "#khaf/Logger": "./src/Structures/Logger.js",
    "#khaf/Bot": "./src/Bot/KhafraBot.js",
    "#khaf/Client": "./src/index.js",
    "#khaf/Timer": "./src/Structures/Timer.js",
    "#khaf/Images": "./src/Images/*",
    "#khaf/structures/*": "./src/Structures/*",
    "#khaf/image/*": "./src/lib/Image/*"
  },
  "dependencies": {
    "@discordjs/builders": "^1.2.0",
    "@discordjs/rest": "^1.2.0",
    "@khaf/magik": "^0.0.3",
    "@khaf/qrcode": "^0.0.11",
    "@napi-rs/canvas": "^0.1.30",
    "@sapphire/shapeshift": "^3.7.0",
    "@skyra/gifenc": "^1.0.0",
    "discord-api-types": "^0.37.11",
    "discord.js": "^14.5.0",
    "entities": "^4.4.0",
    "fast-xml-parser": "^4.0.10",
    "pino": "^8.6.1",
    "postgres": "^3.3.1",
    "twemoji-parser": "^14.0.0",
    "undici": "^5.11.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.16.0",
    "@types/node": "^18.8.1",
    "@types/twemoji-parser": "^13.1.1",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.24.0",
    "husky": "^8.0.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "dev:check": "npx -p typescript tsc",
    "dev:watch": "npx -p typescript tsc --watch",
    "dev:build": "node scripts/build && npx tsc",
    "prod:build": "npx tsc",
    "dev:run": "node --enable-source-maps build/src/index --dev",
    "prod:run": "pm2 start node --name KhafraBot --restart-delay 300000 -- --enable-source-maps build/src/index",
    "lint": "npx eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khafradev/khafra-bot.git"
  },
  "author": "Khafra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khafradev/khafra-bot/issues"
  },
  "homepage": "https://github.com/khafradev/khafra-bot#readme",
  "engines": {
    "node": ">=18.3.0"
  },
  "workspaces": [
    "packages/**"
  ]
}
