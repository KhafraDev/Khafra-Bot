{
  "name": "khafra-bot",
  "version": "1.10.0",
  "description": "Discord Bot",
  "main": "build/src/index.js",
  "type": "module",
  "dependencies": {
    "@discordjs/builders": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "better-sqlite3": "^7.4.3",
    "discord-api-types": "^0.23.1",
    "discord.js": "^13.1.0",
    "entities": "^3.0.1",
    "fast-xml-parser": "^3.19.0",
    "ms": "^3.0.0-canary.1",
    "pg": "^8.7.1",
    "redis": "^3.1.2",
    "search-mdn": "^1.0.0",
    "twemoji-parser": "^13.1.0",
    "undici": "^4.6.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.33",
    "@types/better-sqlite3": "^7.4.0",
    "@types/mime-types": "^2.1.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.9.4",
    "@types/pg": "^8.6.1",
    "@types/redis": "^2.8.28",
    "@types/twemoji-parser": "^13.1.1",
    "@types/ws": "^7.2.7",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.30.0",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "dev:check": "npx -p typescript tsc",
    "dev:watch": "npx -p typescript tsc --watch",
    "dev:build": "node scripts/build && npx tsc",
    "prod:build": "npx tsc",
    "dev:run": "node build/src/index --dev",
    "prod:run": "pm2 start node --name KhafraBot --restart-delay 300000 -- build/src/index --prod",
    "v1.10": "npm run dev:build && node scripts/dump",
    "lint": "npx eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khafradev/khafra-bot.git"
  },
