  "description": "Discord Bot",
  "main": "build/src/index.js",
  "type": "module",
  "dependencies": {
    "@discordjs/builders": "^0.2.0",
    "@khaf/badmeme": "file:src/lib/Packages/BadMeme",
    "adm-zip": "^0.4.16",
    "better-sqlite3": "^7.4.0",
    "discord-api-types": "^0.19.0",
    "discord.js": "^13.0.0-dev.t1627732975.331a9d3",
    "entities": "^2.1.0",
    "fast-xml-parser": "^3.17.6",
    "ms": "^2.1.2",
    "pg": "^8.5.1",
    "redis": "^3.1.2",
    "search-mdn": "^1.0.0",
    "twemoji-parser": "^13.0.0",
    "undici-fetch": "github:Ethan-Arrowood/undici-fetch"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.33",
    "@types/better-sqlite3": "^5.4.1",
    "@types/mime-types": "^2.1.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^15.14.4",
    "@types/pg": "^7.14.10",
    "@types/redis": "^2.8.28",
    "@types/twemoji-parser": "^12.1.0",
    "@types/ws": "^7.2.7",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.30.0",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "dev:check": "npx -p typescript tsc",
    "dev:watch": "npx -p typescript tsc --watch",
    "dev:build": "node scripts/build && npx tsc",
    "prod:build": "npx tsc",
    "dev:run": "node --experimental-json-modules build/src/index",
    "prod:run": "pm2 start npm --name KhafraBot --restart-delay 300000 -- run dev:run",
    "v1.10": "npm run dev:build && node scripts/dump",
    "lint": "npx eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khafradev/khafra-bot.git"
