{
  "name": "khafra-bot",
  "version": "1.10.0",
  "description": "Discord Bot",
  "main": "build/src/index.js",
  "type": "module",
  "imports": {
    "#khaf/utility/*": "./src/lib/Utility/*",
    "#khaf/utility/commands/*": "./src/lib/Packages/*.js",
    "#khaf/utility/Discord.js": "./src/lib/types/Discord.js.js",
    "#khaf/database/*": "./src/Structures/Database/*",
    "#khaf/Command": "./src/Structures/Command.js",
    "#khaf/Interaction": "./src/Structures/Interaction.js",
    "#khaf/Event": "./src/Structures/Event.js"
  },
  "dependencies": {
    "@discordjs/rest": "^0.2.0-canary.0",
    "discord-api-types": "^0.25.2",
    "discord.js": "github:KhafraDev/discord.js",
    "entities": "^3.0.1",
    "esqlite": "^0.0.7",
    "fast-xml-parser": "^4.0.0-beta.8",
    "pg": "^8.7.1",
    "redis": "^4.0.1",
    "sonic-boom": "^2.4.1",
    "twemoji-parser": "^13.1.0",
    "undici": "^4.12.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.4",
    "@types/pg": "^8.6.3",
    "@types/twemoji-parser": "^13.1.1",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "husky": "^7.0.4",
    "typescript": "^4.6.0-dev.20211224"
  },
  "scripts": {
    "dev:check": "npx -p typescript tsc",
    "dev:watch": "npx -p typescript tsc --watch",
    "dev:build": "node scripts/build && npx tsc",
    "prod:build": "npx tsc",
    "dev:run": "node build/src/index --dev",
    "prod:run": "pm2 start node --name KhafraBot --restart-delay 300000 -- build/src/index --prod",
    "v1.10": "npm run dev:build && node scripts/dump",
    "lint": "npx eslint . --ext .ts --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khafradev/khafra-bot.git"
  },
  "author": "Khafra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khafradev/khafra-bot/issues"
  },
  "homepage": "https://github.com/khafradev/khafra-bot#readme",
  "engines": {
    "node": ">=15.4.0"
  },
  "workspaces": [
    "src/lib/Packages/BadMeme",
    "src/lib/Packages/HereWeather",
    "src/lib/Packages/Wikipedia",
    "src/lib/Packages/Spotify",
    "src/lib/Packages/NPM",
    "src/lib/Packages/Pocket",
    "src/lib/Packages/15.ai",
    "src/lib/Packages/MDN",
    "src/lib/Packages/@khaf-builders"
  ]
}
