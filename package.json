{
  "name": "khafra-bot",
  "version": "1.10.0",
  "description": "Discord Bot",
  "main": "build/src/index.js",
  "type": "module",
  "imports": {
    "#khaf/utility/*": "./src/lib/Utility/*",
    "#khaf/utility/commands/*": "./src/lib/Packages/*.js",
    "#khaf/utility/Discord.js": "./src/lib/types/Discord.js.js",
    "#khaf/database/*": "./src/Structures/Database/*",
    "#khaf/migration/*": "./src/lib/Migration/*",
    "#khaf/cache/*": "./src/lib/Cache/*",
    "#khaf/cooldown/*": "./src/Structures/Cooldown/*",
    "#khaf/Command": "./src/Structures/Command.js",
    "#khaf/Interaction": "./src/Structures/Interaction.js",
    "#khaf/Event": "./src/Structures/Event.js",
    "#khaf/Logger": "./src/Structures/Logger.js",
    "#khaf/Bot": "./src/Bot/KhafraBot.js",
    "#khaf/Client": "./src/index.js",
    "#khaf/LRU": "./src/Structures/LRU.js",
    "#khaf/Timer": "./src/Structures/Timer.js"
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.3.0",
    "discord-api-types": "^0.26.0",
    "discord.js": "^14.0.0-dev.1643889786.04502ce",
    "entities": "^3.0.1",
    "esqlite": "^0.0.7",
    "fast-xml-parser": "^4.0.1",
    "node-graceful": "^3.1.0",
    "postgres": "^2.0.0-beta.11",
    "sonic-boom": "^2.6.0",
    "twemoji-parser": "^13.1.0",
    "undici": "^4.13.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.14",
    "@types/twemoji-parser": "^13.1.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "husky": "^7.0.4",
    "typescript": "^4.6.0-dev.20220201"
  },
  "scripts": {
    "dev:check": "npx -p typescript tsc",
    "dev:watch": "npx -p typescript tsc --watch",
    "dev:build": "node scripts/build && npx tsc",
    "prod:build": "npx tsc",
    "dev:run": "node build/src/index --dev",
    "prod:run": "pm2 start node --name KhafraBot --restart-delay 300000 -- build/src/index --prod",
    "v1.10": "npm run dev:build && node scripts/dump",
    "lint": "npx eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khafradev/khafra-bot.git"
  },
  "author": "Khafra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khafradev/khafra-bot/issues"
  },
  "homepage": "https://github.com/khafradev/khafra-bot#readme",
  "engines": {
    "node": ">=15.4.0"
  },
  "workspaces": [
    "packages/**"
  ]
}
